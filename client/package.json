{
  "name": "farm-tracker-web-client",
  "version": "1.10.2",
  "private": true,
  "description": "Track your farm!",
  "homepage": "https://github.com/CandeeGenerations/farm-tracker#readme",
  "bugs": {
    "url": "https://github.com/CandeeGenerations/farm-tracker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CandeeGenerations/farm-tracker.git"
  },
  "license": "MIT",
  "author": "Candee Generations <admin@candeegenerations.com>",
  "scripts": {
    "build": "fnm use; NEXT_PUBLIC_APP_VERSION=$npm_package_version next build",
    "dev": "fnm use; phase run 'NEXT_PUBLIC_APP_VERSION=$npm_package_version next dev --turbopack -p ${PORT-3000}'",
    "eslint": "eslint 'src/**/*.{ts,tsx}'",
    "fix": "pnpm run eslint && pnpm run prettier --cache",
    "preinstall": "npx only-allow pnpm",
    "pcheck": "npm-check -u -i '{@headlessui/react,date-fns,react-day-picker,tailwind-merge,tailwind-variants,tailwindcss}' && pnpm audit",
    "prettier": "prettier --cache --check --write --ignore-unknown .",
    "prettier:ci": "prettier --check .",
    "start": "fnm use; NEXT_PUBLIC_APP_VERSION=$npm_package_version next start"
  },
  "resolutions": {
    "glob": "^10.5.0",
    "js-yaml": "^4.1.1"
  },
  "dependencies": {
    "@headlessui/react": "1.7.19",
    "@headlessui/tailwindcss": "^0.2.2",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
