    "@types/react-dom": "19.2.3",
    "@types/react-tagsinput": "^3.20.6",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "next": "^16.1.1",
    "prettier": "^3.7.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwindcss": "3.4.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  }
