{
  "name": "farm-tracker-web",
  "description": "Track your farm!",
  "repository": "git@github.com:CandeeGenerations/farm-tracker.git",
  "author": "Tyler Candee <tyler@candeegenerations.com>",
  "license": "MIT",
  "version": "0.1.5",
  "private": true,
  "scripts": {
    "dev": "doppler run --command='NEXT_PUBLIC_APP_VERSION=$npm_package_version next dev -p ${PORT-3000}'",
    "build": "npx prisma generate && NEXT_PUBLIC_APP_VERSION=$npm_package_version next build",
    "start": "NEXT_PUBLIC_APP_VERSION=$npm_package_version next start",
    "generate": "npx prisma format && npx prisma generate",
    "db:push": "doppler run --command='npx prisma db push'",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "release": "standard-version -a -- --release-as minor"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@hookform/resolvers": "^2.9.11",
    "@prisma/client": "^4.11.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "avvvatars-react": "^0.4.2",
    "axios": "^1.3.4",
    "change-case": "^4.1.2",
    "chart.js": "^4.2.1",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "morphism": "^1.12.3",
    "next-auth": "^4.20.1",
    "papaparse": "^5.4.0",
    "react-chartjs-2": "^5.2.0",
    "react-content-loader": "^6.2.1",
    "react-datepicker": "^4.10.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.43.5",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@netlify/plugin-nextjs": "^4.32.2",
    "@next/eslint-plugin-next": "^13.2.4",
    "@types/change-case": "^2.3.1",
    "@types/node": "^18.15.3",
    "@types/papaparse": "^5.3.7",
    "@types/react": "^18.0.28",
    "@types/react-datepicker": "^4.10.0",
    "@types/react-dom": "18.0.11",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "next": "^13.2.4",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write"
  }
}
